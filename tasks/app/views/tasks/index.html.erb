<ol>
  <% @tasks.each do |task|%>
    <li>
      <%= task.description %>
      <%= link_to "show priority", task %>
      <%= link_to "delete", task, :method => :delete, :confirm => "are you sure?" %>
      <%= link_to "edit", edit_task_path(task) %>
   </li>
  <% end %>
</ol>
<%= link_to "create a task", new_task_path %>
